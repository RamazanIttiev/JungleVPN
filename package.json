{
  "name": "JungleVPN",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "start": "npm install && docker compose down && docker compose up --build -d && docker compose logs -f",
    "migration:generate": "npm run build && npx typeorm migration:generate db/migrations/Migration -d dist/db/datasource.js",
    "migration:run": "npm run build && npx typeorm migration:run -d dist/db/datasource.js",
    "migration:revert": "npm run build && npx typeorm migration:revert -d dist/db/datasource.js"
  },
  "dependencies": {
    "@grammyjs/menu": "^1.3.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/typeorm": "11.0.0",
    "axios": "^1.12.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "grammy": "^1.38.2",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "tweetnacl": "^1.0.3",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.3",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^11.0.1",
    "@types/bun": "^1.3.1",
    "@types/express": "^5.0.3",
    "@types/node": "^20.12.12",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "zod": "^4.1.12"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "imports": {
    "@bot/*": [
      "src/bot/*"
    ],
    "@remna/*": [
      "src/remna/*"
    ],
    "@payments/*": [
      "src/payments/*"
    ],
    "@assets/*": [
      "src/assets/*"
    ]
  },
  "packageManager": "pnpm@9.13.2+sha1.969cc200a41db98449afee1bfa7578b3ce6ff330"
}
