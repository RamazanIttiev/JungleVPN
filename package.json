{
  "name": "JungleVPN",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:prod": "pm2 stop JungleVPN && bunx nest build && pm2 start JungleVPN",
    "start:dev": "bunx nest start --watch",
    "build": "bunx nest build",
    "typeorm": "typeorm-ts-node-commonjs -d db/migrations/datasource.ts",
    "migration:generate": "bun run build && bun run typeorm -- migration:generate db/migrations/datasource.ts",
    "migration:run": "bun run typeorm -- migration:run",
    "migration:revert": "bun run typeorm -- migration:revert",
    "migration:show": "bun run typeorm -- migration:show"
  },
  "dependencies": {
    "@biomejs/biome": "^2.2.3",
    "@grammyjs/menu": "^1.3.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "11.0.0",
    "axios": "^1.12.2",
    "axios-cookiejar-support": "^6.0.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "grammy": "^1.38.2",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "tough-cookie": "^6.0.0",
    "tweetnacl": "^1.0.3",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "11.1.6",
    "@types/express": "^5.0.3",
    "@types/node": "^20.12.12",
    "dotenv": "^17.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
